{
  "scripts": {
    "start": "concurrently \"npm run start-frontend\" \"npm run start-backend\"",
    "start-frontend": "cd school-management-frontend && npm start",
    "start-backend": "cd school-management-backend && npm start",
    "build": "npm run build-frontend && npm run build-backend",
    "build-frontend": "cd school-management-frontend && npm run build",
    "build-backend": "cd school-management-backend && npm run build"
  }
}
